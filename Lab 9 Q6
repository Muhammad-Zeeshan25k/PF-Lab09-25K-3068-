#include <stdio.h>

float taxt(int income,float *tax, float *netincome){
    if(income>=0 && income<=20000){
        *tax=0.0;
    }
    else if (income>20000 && income<=50000)
    *tax=income*0.1;
    else if (income>50000){
        *tax=income*0.2;
    }
    *netincome=income-*tax;
    return 1.0;
}

int main() {
    int income;
    float Mtax,Mnetincome;
    printf("Enter the Income :");
    scanf("%d",&income);
    taxt(income,&Mtax,&Mnetincome);
    printf("The Net income is %.2f\n",Mnetincome);
    printf("The Tax is :%.2f\n",Mtax);
    return 0;
}
